{
  "plugins": [ "prettier-plugin-tailwindcss", "prettier-plugin-classnames" ],
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 4,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "tailwindPreserveWhitespace": true,
  "htmlWhitespaceSensitivity": "ignore",
}